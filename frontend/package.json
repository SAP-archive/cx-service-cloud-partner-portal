{
  "name": "partner-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "dev": "ng serve --configuration=local",
    "test:dev": "ng test --code-coverage --karma-config=karma.dev.conf.js",
    "test:docker": "./run-tests-in-docker.sh",
    "posttest": "npm run remove-absolute-path-coverage",
    "remove-absolute-path-coverage": "sed -i.bak 's/SF:.*app\\/frontend-build\\//SF:/g' coverage/lcov.info && rm coverage/lcov.info.bak",
    "lint": "ng lint",
    "e2e:localhost": "ng e2e --protractorConfig=./e2e/local.protractor.conf.js",
    "e2e:et:remote": "protractor ./e2e/remote.protractor.conf.js --params.useEnv='ET'",
    "e2e:et:local": "webdriver-manager update --chrome --gecko=false --standalone=false && protractor ./e2e/local.protractor.conf.js --params.useEnv='ET'",
    "e2e:qt:remote": "protractor ./e2e/remote.protractor.conf.js --params.useEnv='QT'",
    "e2e:qt:local": "webdriver-manager update --chrome --gecko=false --standalone=false && protractor ./e2e/local.protractor.conf.js --params.useEnv='QT'",
    "e2e:sb:remote": "protractor ./e2e/remote.protractor.conf.js --params.useEnv='SB'",
    "e2e:sb:local": "webdriver-manager update --chrome --gecko=false --standalone=false && protractor ./e2e/local.protractor.conf.js --params.useEnv='SB'",
    "e2e:de:remote": "protractor ./e2e/remote.protractor.conf.js --params.useEnv='DE'",
    "e2e:de:local": "webdriver-manager update --chrome --gecko=false --standalone=false && protractor ./e2e/local.protractor.conf.js --params.useEnv='DE'",
    "e2e:eu:remote": "protractor ./e2e/remote.protractor.conf.js --params.useEnv='EU'",
    "e2e:eu:local": "webdriver-manager update --chrome --gecko=false --standalone=false && protractor ./e2e/local.protractor.conf.js --params.useEnv='EU'",
    "e2e:us:remote": "protractor ./e2e/remote.protractor.conf.js --params.useEnv='US'",
    "e2e:us:local": "webdriver-manager update --chrome --gecko=false --standalone=false && protractor ./e2e/local.protractor.conf.js --params.useEnv='US'"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ngrx/effects": "^8.5.2",
    "@ngrx/entity": "^8.5.2",
    "@ngrx/schematics": "^8.5.2",
    "@ngrx/store": "^8.5.2",
    "@ngrx/store-devtools": "^8.5.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/jasminewd2": "^2.0.6",
    "angular-uuid4": "^0.3.1",
    "file-saver": "^2.0.2",
    "hammerjs": "^2.0.8",
    "jasmine-marbles": "^0.6.0",
    "ldclient-js": "^2.10.2",
    "ngrx-store-localstorage": "^8.0.0",
    "ngx-file-drop": "^8.0.8",
    "ngx-store": "^2.1.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.9.0",
    "uuid": "^3.3.3",
    "xregexp": "^4.2.4",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/cli": "~8.0.6",
    "@angular/compiler-cli": "~8.0.3",
    "@angular/language-service": "^8.2.3",
    "@types/file-saver": "^2.0.1",
    "@types/googlemaps": "^3.37.4",
    "@types/jasmine": "~3.5.0",
    "@types/node": "^8.10.52",
    "@types/uuid": "^3.4.6",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-parameterized": "^0.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasminewd2": "^2.2.0",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
